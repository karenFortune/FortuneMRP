                var html = '<label class="">Address&nbsp;</label> <select class="form-control pk" id="caja_direccion_pk_dir" >';
                //var html = '<select class="form-control " id="caja_direccion_pk" >';
                $.each(result, function (key, item) {
                    html += '<option  value="' + item.id_direccion + '">' + item.nombre + '</option>';
                });
                html += '</select>';
                $("#div_shipto_dir").html(html);
                $("#loading_pk_dir").css('display', 'none');
            }
        });
    }
    function obtener_customers_pk_dir() {
        $.ajax({
            url: '/Shipping/buscar_clientes_select',
            contentType: 'application/json',
            dataType: 'json',
            type: 'post',
            success: function (result) {
                var html = ''; //'<label class="">PO&nbsp;</label> <select class="form-control js-example-basic-single" id="caja_pedido" >';
                // += '<option  value="0">Select</option>';
                $.each(result, function (key, item) {
                    html += '<option  value="' + item.id_customer + '">' + item.customer + '</option>';
                });
                //html += '</select>';
                $(".customer_dir").append(html);
            }
        });
    }
    function llenar_estilos_fantasy() {
        $("#loading_pk_dir").css('display', 'inline');
        $.ajax({
            url: '/Shipping/buscar_estilos_fantasy_select',
            contentType: 'application/json',
            dataType: 'json',
            type: 'post',
            success: function (result) {
                var html = '';
                $.each(result, function (key, item) {
                    html += '<tr>';
                    html += '<td style="visibility:hidden; border: none; width:5%;">' + item.id_estilo + '</td>';
                    html += '<td style=" width:30%;">' + item.estilo + '</td>';
                    html += '<td style=" width:35%;">' + item.descripcion + '</td>';
                    html += '<td style=" width:30%;">' + item.color + '</td>';
                    html += '</tr>';
                });
                //html += '</select>';
                $(".rows_estilos_dir").html(html);
                $("#loading_pk_dir").css('display', 'none');
            }
        });
    }
    function obtener_labels_tabla_dir() {
        labels = '';
        if ($("#check_ucclabels_dir").val() != '0') {
            $('#tabla_ucc_labels_dir tbody tr').each(function () {
                labels += "*" + $(this).find('td:eq(0) input').val();
            });
        } else {
            labels = 'N/A';
        }
    }
    $(document).on("dblclick", "#tabla_estilos_dir tr", function () {
        var row = this.rowIndex;
        var id = $('#tabla_estilos_dir tr:eq(' + row + ') td:eq(0)').html();
        var descripcion = $('#tabla_estilos_dir tr:eq(' + row + ') td:eq(1)').html() + ' ' + $('#tabla_estilos_dir tr:eq(' + row + ') td:eq(2)').html();
        agregar_fila_dir(id + '&' + descripcion);
    });
    function agregar_fila_dir(informacion) {
        indice_dir++;
        // obtener_labels_tabla_dir();
        var data = informacion.split("&");
        var html = '<tr>' +
            '<td style="width:1px; border: none;">' + data[0] + '</td>' + //ID
            '<td>' + data[1] + '</td>';//ESTILO
        html += '<td><input   maxlength="15" class="validacion form-control " value="0" style=" padding: 1px 2px;" /></td>'//posum
        html += '<td><input    maxlength="6" class="numeric form-control " value="0" style=" padding: 1px 2px;" /></td>'//TALLAS
        html += '<td><input    maxlength="6" class="numeric form-control " value="0" style=" padding: 1px 2px;" /></td>'//TALLAS
        html += '<td><input    maxlength="6" class="numeric form-control " value="0" style=" padding: 1px 2px;" /></td>'//TALLAS
        html += '<td><input    maxlength="6" class="numeric form-control " value="0" style=" padding: 1px 2px;" /></td>'//TALLAS
        html += '<td><input    maxlength="6" class="numeric form-control " value="0" style=" padding: 1px 2px;" /></td>'//TALLAS
        html += '<td><button type="button"  title="Delete row" class="btn btn-light deleteRowDir"><b>-</b></button></td>';
        html += '<td style="visibility:hidden; border: none; width:1px;">' + indice_dir + '</td>';//INDICE
        estilos_seleccionados_dir++;
        $('#div_tabla_temporal_dir').css('visibility', 'visible');
        $('.rows_tabla_temporal_dir').append(html);
    }
    $(document).on("click", ".deleteRowDir", function () {
        // var temporal_index = $(this).closest("tr").find('td:eq(11)').html();
        $(this).closest("tr").remove();
        estilos_seleccionados_dir--;
        if (estilos_seleccionados_dir != 0) {
            $('#div_tabla_temporal_dir').css('visibility', 'visible');
        } else {
            $('#div_tabla_temporal_dir').css('visibility', 'hidden');
        }
    });

    var estilo, label, sm, md, lg, xl, xxl, index_dir, cantidad;
    function revisar_informacion_pk_dir(event) {
        estilo = ''; label = ''; sm = ''; md = ''; lg = ''; xl = ''; xxl = ''; index_dir = ''; cantidad = '';
        $("#loading_pk_dir").css('display', 'inline');
        var error = 0; label = '';
        if ($("#caja_pedido_pk_dir").val() == '') {
            $(this).css('border', '2px solid #e03f3f');
            error++;
        } else {
            $(this).css('border', '1px solid #cccccc');
        }
        $(".check_item_labels_dir").each(function () {
            if ($(this).val() == '') {
                $(this).css('border', '2px solid #e03f3f');
                error++;
            } else {
                $(this).css('border', '1px solid #cccccc');
            }
        });
        labels = '';
        if ($("#check_ucclabels_dir").val() != '0') {
            $('#tabla_ucc_labels_dir tbody tr').each(function () {
                labels += "*" + $(this).find('td:eq(0) input').val();
            });
        } else {
            labels = 'N/A';
        }

        $('#tabla_temporal_dir tbody tr').each(function () {
            //label_dir
            var noceros = 0;
            cantidad += '*';
            for (var i = 3; i < 8; i++) {
                if ($(this).find('td:eq(' + i + ') input ').val() != '' && $(this).find('td:eq(' + i + ') input ').val() != '0') {
                    noceros++;
                }
                if ($(this).find('td:eq(' + i + ') input ').val() == '') {
                    cantidad += '&0';
                } else {
                    cantidad += '&' + $(this).find('td:eq(' + i + ') input ').val();
                }
            }
            if (noceros == 0) { error++; }
            estilo += '*' + $(this).find('td:eq(0) ').html();
            label += '*' + $(this).find('td:eq(2) input ').val();
            /*sm += '*'+$(this).find('td:eq(3) input ').val();
            md += '*'+$(this).find('td:eq(4) input ').val();
            lg += '*'+$(this).find('td:eq(5) input ').val();
            xl += '*'+$(this).find('td:eq(6) input ').val();
            xxl += '*'+$(this).find('td:eq(7) input ').val();*/
            index_dir += '*' + $(this).find('td:eq(9) ').html();
        });

        if (error == 0) {
            $("#loading_pk_dir").css('display', 'none');
            enviar_datos_pk_dir();
        } else {
            $("#loading_pk_dir").css('display', 'none');
            event.preventDefault();
            alertify.alert('Error!', 'Check empty fields/quantities, please');
            return false;
        }
    }
    function enviar_datos_pk_dir() { //ENVIAR DATOS PACKING LIST- INFORMACIÓN GENERAL, NOS REGRESA LOS ESTILOS DE ESA ORDEN
        $("#loading_pk_dir").css('display', 'inline');
        var seal, replacement,pk=0;
        if ($("#caja_seal_pk_dir").val() == '') { seal = 'N/A'; } else { seal = $("#caja_seal_pk_dir").val(); }
        if ($("#caja_replacement_pk_dir").val() == '') { replacement = 'N/A'; } else { replacement = $("#caja_replacement_pk_dir").val(); }
        if ($("#caja_sin_pk").is(":checked")) { pk = 1; } 
        $.ajax({
            url: '/Shipping/guardar_pk_directo_fantasy',
            data: "{'customer':'" + $("#caja_customer_pk_dir").val() + "','pedido':'" + $("#caja_pedido_pk_dir").val() + "','address':'" + $("#caja_direccion_pk_dir").val() + "','driver':'" + $("#caja_conductor_pk_dir").val() + "','container':'" + $("#caja_contenedor_pk_dir").val() + "','seal':'" + seal + "','replacement':'" + replacement + "','tipo':'" + $("#caja_tipo_pk_dir").val() + "','labels':'" + labels + "','type_labels':'" + $("#check_ucclabels_dir").val() + "','num_envio':'" + $("#caja_num_envio_dir").val() + "','tipo_packing':'8','pk_id':'" + pk + "'}",
            contentType: 'application/json',
            dataType: 'json',
            type: 'post',
            success: function (jsonData) {
                enviar_estilos_pk_dir();
            }
        });
    }

    function enviar_estilos_pk_dir() {
        //var estilo, label, sm, md, lg, xl,xxl,index_dir,cantidad;
        $("#loading_pk").css('display', 'inline');
        $.ajax({
            url: '/Shipping/guardar_pk_estilos_directo_fantasy',
            data: "{'estilo':'" + estilo + "','po_sum':'" + label + "','index':'" + index_dir + "','cantidad':'" + cantidad + "'}",
            contentType: 'application/json',
            dataType: 'json',
            type: 'post',
            success: function (result) {
                var error = result;
                $("#loading_pk_dir").css('display', 'none');
                if (error == 1) {
                    event.preventDefault();
                    alertify.alert('Error!', 'Check the quantities, please');
                    //alert("ERRORRRR");
                } else {
                    alertify.success('Succes!');
                    alertify.set('notifier', 'position', 'top-right');
                    //alert("Si se guardó");
                    redireccionarPaginaDir();
                }
            }
        });
    }
    function redireccionarPaginaDir() { window.location = '/Shipping/Index'; }
</script>

