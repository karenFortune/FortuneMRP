    var inventario, talla, cantidades, qty, cajas, index_re, categoria, item;
    function guardar_items_return(event) {
        var error = 0;
        inventario = ''; talla = ''; cantidades = ''; qty = ''; cajas = ''; index_re = '';categoria = ''; item = '';
        var total_tallas_tabla = 0;
        $.each(lista_tallas_ret, function (key, item) { total_tallas_tabla++; talla += '*' + item.id_talla; });
        $('#tabla_temporal_pedido_re tbody tr').each(function () { 
            inventario += "*" + $(this).find('td:eq(0)').html();
            var disponible = parseFloat($(this).find('td:eq(2)').html());
            var con_numero = 0;
            var i;
            cantidades+= '*';
            for (i = 3; i < (total_tallas_tabla + 3); i++) {                
                if ($(this).find('td:eq(' + i + ') input').val() != '' && $(this).find('td:eq(' + i + ') input').val() != 0) {
                    con_numero++;
                }                    
                if ($(this).find('td:eq(' + i + ') input').val() == '') {
                    cantidades += '&0';
                } else {
                    cantidades += '&' + $(this).find('td:eq(' + i + ') input').val();
                }
            }
            var cantidad_temporal = $(this).find('td:eq(' + i + ') input').val(); i++;
            qty += '*' + cantidad_temporal;
            cajas+= '*' + $(this).find('td:eq(' + i + ') input').val(); i++; i++;
            index_re += '*' + $(this).find('td:eq(' + i + ') ').html(); i++;
            var categoria_temporal = $(this).find('td:eq(' + i + ') ').html(); i++;
            categoria += '*' + categoria_temporal;
            item += '*' + $(this).find('td:eq(' + i + ') ').html(); i++;
            if ((con_numero == 0 && cantidad_temporal == '0') ||(con_numero == 0 && cantidad_temporal == '') ) {
                error++;
            }           
        });        

        if (error == 0) {
            enviar_datos_pk_re();           
        } else {
            $("#loading_pk_re").css('display', 'none');
            event.preventDefault();
            alertify.alert('Error!', 'Check empty fields, please');
            return false;
        }
    }
     function enviar_datos_pk_re() { //ENVIAR DATOS PACKING LIST- INFORMACIÓN GENERAL, NOS REGRESA LOS ESTILOS DE ESA ORDEN
        $("#loading_pk_re").css('display', 'inline');
        var seal_re, replacement_re,pk=0;
        if ($("#caja_seal_pk_re").val() == '') { seal_re = 'N/A'; } else { seal_re = $("#caja_seal_pk_re").val(); }
        if ($("#caja_replacement_pk_re").val() == '') { replacement_re = 'N/A'; } else { replacement_re = $("#caja_replacement_pk_re").val(); }    
        if ($("#caja_sin_pk").is(":checked")) { pk = 1; } 
        $.ajax({
            url: '/Shipping/guardar_pk',
            data: "{'pedido':'" + $("#caja_pedido_pk_re").val() + "','address':'" + $("#caja_direccion_pk_re").val() + "','driver':'" + $("#caja_conductor_pk_re").val() + "','container':'" + $("#caja_contenedor_pk_re").val() + "','seal':'" + seal_re+ "','replacement':'" + replacement_re+ "','tipo':'" + $("#caja_tipo_pk_re").val() + "','labels':'N/A','type_labels':'N/A','dcs':'0','num_envio':'" + $("#caja_num_envio_re").val() + "','tipo_packing':'5','pk_id':'" + pk + "'}",
            contentType: 'application/json',
            dataType: 'json',
            type: 'post',
            success: function (jsonData) {
                enviar_datos_inventario_re();
            }
        });
    }
    function enviar_datos_inventario_re() { //ENVIAR DATOS PACKING LIST- INFORMACIÓN GENERAL, NOS REGRESA LOS ESTILOS DE ESA ORDEN
       $("#loading_pk_re").css('display', 'none');
            $.ajax({
                url: '/Shipping/guardar_items_retorno',
                data: "{'pedido':'" + $("#caja_pedido_pk_re").val() + "','inventario':'" +inventario + "','cantidad':'" +cantidades + "','categoria':'" +categoria + "','talla':'" + talla + "','item':'" + item + "','caja':'"+cajas+"','qty':'"+qty+"','indice':'"+index_re+"'}",
                contentType: 'application/json',
                dataType: 'json',
                type: 'post',
                success: function (result) {
                   $("#loading_pk_re").css('display', 'none');               
                   alertify.success('Succes!');
                   alertify.set('notifier', 'position', 'top-right');
                   redireccionarPagina_re();                         
                }
            });
    }
