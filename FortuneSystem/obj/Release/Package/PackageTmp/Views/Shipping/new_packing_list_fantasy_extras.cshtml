            data: "{'summary':'" + summary_fx + "','caja':'" + caja_fx + "','cantidad':'" + cantidades_fx + "','tipo':'" + tipo_fx + "'}",
            contentType: 'application/json',
            dataType: 'json',
            type: 'post',
            success: function (result) {  
                var error = result;                
                if (error == 1) {
                    $("#loading_pk_fx").css('display', 'none');
                     alertify.alert('Error!', 'Check the quantities, please');
                    event.preventDefault();
                    return false;
                   
                } else {
